{
  "name": "tbphp/laravel-queue-rocketmq",
  "description": "laravel 8 queue rocketmq implements",
  "authors": [
    {
      "name": "tbphp",
      "email": "tbphp@admin.net"
    }
  ],
  "require": {
    "php": ">=7.4",
    "illuminate/queue": "^8.0",
    "ext-json": "*",
    "ext-curl": "*",
    "aliyunmq/mq-http-sdk": "^1.0",
    "guzzlehttp/guzzle": "^7.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": ">=6.0.0",
    "vlucas/phpdotenv": "^3.3",
    "mockery/mockery": "^1.2.3"
  },
  "autoload": {
    "psr-4": {
      "Tbphp\\LaravelQueueRocketMQ\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tbphp\\LaravelQueueRocketMQ\\Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  },
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "Tbphp\\LaravelQueueRocketMQ\\LaravelQueueRocketMQServiceProvider"
      ]
    }
  }
}
